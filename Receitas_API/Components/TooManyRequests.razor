@page "/429"


  <style>
    /* Styles for the error card */
    .error-card {
      background-color: #fff; /* White background */
      border-radius: 10px; /* Rounded corners */
      box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1); /* Shadow effect */
      padding: 20px; /* Padding around the content */
      max-width: 400px; /* Limit card width */
      margin: 0 auto 20px; /* Center card and add space below */
      position: relative; /* Positioning for close button */
      animation: fadeIn 0.5s ease; /* Animation for entrance */
    }

    /* Optional: Add more specific styles for different error types */
    .error-card.error-429 {
      /* You can customize this further if needed */
    }

    /* Styles for the error message */
    .error-message {
      color: #f44336; /* Red color for error message */
      font-weight: normal;
      margin-bottom: 10px; /* Spacing below the message */
    }

    /* Optional: Style the close button */
    .error-card .close {
      cursor: pointer;
      position: absolute;
      top: 10px;
      right: 10px;
      font-size: 20px;
      color: #999; /* Light grey color */
      transition: transform 0.3s ease; /* Smooth transition */
    }

    /* Optional: Hover effect for close button */
    .error-card .close:hover {
      color: #777; /* Darker grey on hover */
    }

    /* Animation keyframes */
    @@keyframes fadeIn {
      0% { opacity: 0; transform: translateY(-20px); }
      100% { opacity: 1; transform: translateY(0); }
    }
  </style>
<div class="error-card error-429">
    <span class="close" onclick="this.parentElement.style.display='none';">&times;</span> <!-- Close button -->
    <p class="error-message"><strong>Error 429:</strong> Maximum monthly number of attempts exceeded.</p> <!-- Error message -->
</div>

